{
  "name": "github-avatar",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-hook"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "npx @directus/extensions-sdk build",
    "dev": "npx @directus/extensions-sdk build -w --no-minify",
    "link": "npx @directus/extensions-sdk link",
    "validate": "npx @directus/extensions-sdk validate"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "16.0.1"
  }
}
